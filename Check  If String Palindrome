// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
 bool ispalindrome(string str){
     int n = str.length();
     for(int i=0;i<str.length();i++){
         if(str[i]!=str[n-i-1]){
             return false;
         }
     }
     return true;
 }

int main() {
    // check if string is palindrome or not
    string str;
    cin>>str;
        if(ispalindrome(str)){
            cout<< "it is a palindrome string"<<endl;
        }
        else{
        cout<<" is is not a palindrome string"<<endl;}
    return 0;
}
